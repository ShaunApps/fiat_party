


  <div class="container">
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
      <div class="panel panel-success">
        <div class="panel-heading">
          <h3 class="panel-title">Total USD Balance</h3>
        </div>
        <div class="panel-body">
          <strong>$<%= USD_TOTAL %></strong>
        </div>
      </div>
    </div>


<div class="col-md-12">
<table class="table table-border table-hovered table-striped">
  <thead>
    <tr class="info">
      <th>Asset Name</th>
      <th>Unit Price</th>
      <th>Asset Amount</th>
      <th>Dollar Amount</th>
    </tr>
  </thead>

  <tbody>
    <% for (var asset of assets) { %>
      <tr class="warning">
          <td><h4><%= asset.name %></h4></td>
          <td>$<%= asset.unit_price %></td>
          <td><%= asset.asset_amount %></td>
          <td>$<%= asset.unit_price * asset.asset_amount %></td>
      </tr>
  <% } %>
  </tbody>
</table>
</div>
</div>
